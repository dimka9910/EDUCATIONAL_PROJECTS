<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="324.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.spbstu.java.ui.controller.statistic.Top5ItemsController">
   <TableView fx:id="itemsTable" layoutX="14.0" layoutY="59.0" prefHeight="200.0" prefWidth="333.0">
      <columns>
         <TableColumn fx:id="nameColumn" prefWidth="162.0" text="Name"/>
         <TableColumn fx:id="amountColumn" prefWidth="170.0" text="Amount"/>
      </columns>
   </TableView>
   <Button fx:id="closeButton" layoutX="425.0" layoutY="67.0" mnemonicParsing="false" onAction="#close"
           prefHeight="32.0" prefWidth="109.0" text="Close"/>
   <Button fx:id="showButton" layoutX="425.0" layoutY="227.0" mnemonicParsing="false" onAction="#show" prefHeight="32.0"
           prefWidth="109.0" text="Show"/>
   <DatePicker fx:id="startDatePicker" layoutX="405.0" layoutY="128.0"/>
   <DatePicker fx:id="endDatePicker" layoutX="405.0" layoutY="175.0"/>
   <Label layoutX="353.0" layoutY="132.0" text="Start:">
      <font>
         <Font name="Times New Roman" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="353.0" layoutY="179.0" text="End:">
      <font>
         <Font size="14.0"/>
      </font>
   </Label>
   <Label layoutX="205.0" layoutY="24.0" text="Top 5 items for date range">
      <font>
         <Font name="Times New Roman" size="18.0"/>
      </font>
   </Label>
</AnchorPane>
