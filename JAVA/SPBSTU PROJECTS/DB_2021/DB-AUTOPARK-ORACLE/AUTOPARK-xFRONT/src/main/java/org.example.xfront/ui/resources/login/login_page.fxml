<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="150.0" prefWidth="279.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.spbstu.java.ui.controller.LoginController">
   <Button fx:id="signInButton" layoutX="96.0" layoutY="101.0" mnemonicParsing="false" onAction="#signIn"
           prefHeight="35.0" prefWidth="87.0" text="Sign in">
      <font>
         <Font name="Times New Roman" size="14.0"/>
      </font>
   </Button>
   <Label layoutX="24.0" layoutY="22.0" prefHeight="26.0" prefWidth="67.0" text="Login:">
      <font>
         <Font name="Times New Roman" size="14.0"/>
      </font>
   </Label>
   <Label layoutX="24.0" layoutY="55.0" prefHeight="26.0" prefWidth="67.0" text="Password:">
      <font>
         <Font name="Times New Roman" size="14.0"/>
      </font>
   </Label>
   <TextField fx:id="loginTextField" layoutX="91.0" layoutY="23.0"/>
   <PasswordField fx:id="passwordTextField" layoutX="91.0" layoutY="56.0"/>
</AnchorPane>
